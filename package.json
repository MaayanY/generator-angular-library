{
  "name": "generator-angular2-module",
  "version": "1.0.0",
  "description": "Generate an angular2 module scaffolding suitable for publishing a re-usable module to npm",
  "homepage": "",
  "author": {
    "name": "Matt Lewis",
    "email": "matthew.lewis@socialsignin.co.uk",
    "url": "https://mattlewis.me"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "angular2",
    "module",
    "yeoman-generator"
  ],
  "dependencies": {
    "yeoman-generator": "^0.23.0",
    "chalk": "^1.0.0",
    "yosay": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^2.1.0",
    "eslint-config-xo-space": "^0.13.0"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "rules": {
      "space-before-function-paren": 0,
      "padded-blocks": 0,
      "no-implicit-coercion": 0,
      "handle-callback-err": 0
    }
  },
  "repository": "git@github.com:mattlewis92/generator-angular2-module.git",
  "license": "MIT",
  "scripts": {
    "lint": "eslint generators",
    "prepublish": "npm run lint"
  }
}
